{
  "realm": "research",
  "enabled": true,
  "displayName": "Keycloak Research Realm",
  "users": [
    {
      "username": "test-user",
      "enabled": true,
      "emailVerified": true,
      "email": "test-user@example.com",
      "firstName": "Test",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "requiredActions": [],
      "realmRoles": ["offline_access", "default-roles-research", "uma_authorization"]
    }
  ],
  "clients": [
    {
      "clientId": "research-migration-from-legacy",
      "enabled": true,
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "serviceAccountsEnabled": false,
      "rootUrl": "http://localhost:4000",
      "redirectUris": ["http://localhost:4000/*"],
      "webOrigins": ["*"]
    }
  ],
  "components": {
    "org.keycloak.storage.UserStorageProvider": [
      {
        "id": "legacy-user-storage",
        "name": "Legacy User Storage",
        "providerId": "legacy-user-storage",
        "config": {
          "priority": ["0"],
          "cachePolicy": ["DEFAULT"],
          "batchSizeForSync": ["1000"],
          "maxLifespan": ["-1"],
          "evictionHour": ["0"],
          "evictionMinute": ["0"],
          "evictionDay": [""],
          "importEnabled": ["true"],
          "legacyBaseUrl": ["http://host.docker.internal:4000/"]
        }
      }
    ]
  }
}
